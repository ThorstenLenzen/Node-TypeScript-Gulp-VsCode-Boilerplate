{
    "version": "0.2.0",
    "configurations": [
        {
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/app/app.ts",
			"stopOnEntry": true,
            "cwd": "${workspaceRoot}/app",
            "env": {
                "NODE_ENV": "development"
            },
            "console": "internalConsole",
            "sourceMaps": true
		},
        {
			"name": "Attach to Node",
			"type": "node",
			"request": "attach",
			"address": "localhost",
			"port": 5858,
			"restart": false
		}
    ]
}